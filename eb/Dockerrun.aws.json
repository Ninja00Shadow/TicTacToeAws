{
    "AWSEBDockerrunVersion": 3,
    "containerDefinitions": [
      {
        "name": "backend",
        "image": "ninjashadow/tictactoe:backend",
        "update": "true",
        "essential": true
      },
      {
        "name": "frontend",
        "image": "ninjashadow/tictactoe:frontend",
        "update": "true",
        "essential": true,
        "memory": 512,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": [
          "backend"
        ]
      }
    ]
  }
  