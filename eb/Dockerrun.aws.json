{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "backend",
        "image": "./TicTacToe backend",
        "essential": true,
        "memory": 512,
        "environment": [
          {
            "name": "BACKEND_PORT",
            "value": "8000"
          }
        ],
        "portMappings": [
          {
            "hostPort": 8000,
            "containerPort": 8000
          }
        ]
      },
      {
        "name": "frontend",
        "image": "./tictactoe-frontend",
        "essential": true,
        "memory": 512,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": [
          "backend"
        ],
        "environment": [
          {
            "name": "FRONTEND_PORT",
            "value": "80"
          },
          {
            "name": "BACKEND_IP",
            "value": "0.0.0.0"
          }
        ]
      }
    ]
  }
  